<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="BackGround">#004299</Color>
    <Color x:Key="SelectedBackGround">#005ACF</Color>
    <Color x:Key="SelectedText">#FFC745</Color>
    <Color x:Key="NormalText">#ffff</Color>
    <Color x:Key="WorningColor">#FF0E00</Color>
    <Color x:Key="DonColor">#00AB14</Color>
    <Color x:Key="ViewMainColor">#CDD7FE</Color>
    <Color x:Key="ViewItemColor">#00627A</Color>
    <Color x:Key="SideBarColor">#003947</Color>


    <SolidColorBrush x:Key="BackGroundBrush" Color="{StaticResource BackGround}" />
    <SolidColorBrush x:Key="SelectedBackGroundBrush" Color="{StaticResource SelectedBackGround}" />
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource NormalText}" />
    <SolidColorBrush x:Key="SelectedTextBrush" Color="{StaticResource SelectedText}" />
    <SolidColorBrush x:Key="WorningBrush" Color="{StaticResource WorningColor}" />
    <SolidColorBrush x:Key="DonBrush" Color="{StaticResource DonColor}" />
    <SolidColorBrush x:Key="ViewMainBackGroundBrush" Color="{StaticResource ViewMainColor}" />
    <SolidColorBrush x:Key="ViewItemBackGroundBrush" Color="{StaticResource ViewItemColor}" />
    <SolidColorBrush x:Key="SideBarBackGroundBrush" Color="{StaticResource SideBarColor}" />


</ResourceDictionary>